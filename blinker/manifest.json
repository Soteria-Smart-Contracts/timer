{
    "manifest_version": 3,
    "name": "Blinker Game",
    "version": "1.0",
    "description": "A game to encourage blinking.",
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
      }
  }